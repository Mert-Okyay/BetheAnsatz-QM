Cell[BoxData[
 RowBox[{
  RowBox[{"tensorTrace", "[", 
   RowBox[{
    RowBox[{"arr_", "?", "ArrayQ"}], ",", "op_"}], "]"}], ":=", " ", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"i", ",", "trace"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"trace", " ", "=", 
      RowBox[{"Join", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"arr", "[", 
          RowBox[{"[", "1", "]"}], "]"}], "}"}], ",", 
        RowBox[{"Array", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"Identity", "[", 
            RowBox[{"{", 
             RowBox[{"0", ",", "0"}], "}"}], "]"}], "&"}], ",", " ", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{"Length", "@", "arr"}], "-", "1"}], "}"}]}], "]"}]}], 
       "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"i", "=", "1"}], ",", " ", 
       RowBox[{"i", "<", 
        RowBox[{"Length", "@", "arr"}]}], ",", 
       RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{
         RowBox[{"trace", "[", 
          RowBox[{"[", 
           RowBox[{"i", "+", "1"}], "]"}], "]"}], " ", "=", " ", 
         RowBox[{"Flatten", "@", 
          RowBox[{"(", 
           RowBox[{"op", "[", 
            RowBox[{
             RowBox[{"trace", "[", 
              RowBox[{"[", "i", "]"}], "]"}], ",", 
             RowBox[{"arr", "[", 
              RowBox[{"[", 
               RowBox[{"i", "+", "1"}], "]"}], "]"}]}], "]"}], ")"}]}]}], " ",
         ";"}]}], "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"trace", "[", 
      RowBox[{"[", 
       RowBox[{"-", "1"}], "]"}], "]"}]}]}], "]"}]}]], "Input",
