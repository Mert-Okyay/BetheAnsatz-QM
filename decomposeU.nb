Cell["\<\
We represent a state with nn_ down spins at coordinates given by an array \
pos_, and the total number of spins is dim:\
\>", "Subsubsection",
 CellChangeTimes->{{3.8406345419789877`*^9, 3.8406345850490828`*^9}, 
   3.8407201148896523`*^9},ExpressionUUID->"c88e1ce2-d950-4f81-b365-\
2c862c3ecf74"],

Cell[BoxData[
 RowBox[{
  SubscriptBox[
   TemplateBox[{
     RowBox[{
       RowBox[{"nn_", "?", "IntegerQ"}], ",", 
       RowBox[{"pos_", "?", "VectorQ"}]}]},
    "Ket"], 
   RowBox[{"dim_", "?", "IntegerQ"}]], ":=", " ", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"n", ",", "i"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{"Catch", "[", "\[IndentingNewLine]", 
     RowBox[{"If", "[", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"Or", "[", 
        RowBox[{
         RowBox[{"nn", ">", "dim"}], ",", " ", 
         RowBox[{
          RowBox[{"Length", "@", "pos"}], ">", "dim"}]}], "]"}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{
       "Throw", "[", 
        "\"\<Number of down spins or length of position array larger than \
chain length.\>\"", "]"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"nn", ">", "dim"}], ",", 
          RowBox[{"n", "=", "dim"}], ",", 
          RowBox[{"n", "=", "nn"}]}], "]"}], ";", " ", "\[IndentingNewLine]", 
        "\[IndentingNewLine]", 
        RowBox[{"state", " ", "=", " ", 
         RowBox[{"Array", "[", 
          RowBox[{"0", ",", " ", 
           RowBox[{"{", "dim", "}"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"For", "[", 
         RowBox[{
          RowBox[{"i", "=", "1"}], ",", " ", 
          RowBox[{"i", "<=", "dim"}], ",", 
          RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"state", "[", 
            RowBox[{"[", "i", "]"}], "]"}], "=", 
           RowBox[{"If", "[", 
            RowBox[{
             RowBox[{"MemberQ", "[", 
              RowBox[{"pos", ",", " ", "i"}], "]"}], ",", 
             TemplateBox[{"down"},
              "Ket"], ",", 
             TemplateBox[{"up"},
              "Ket"]}], " ", "]"}]}]}], "\[IndentingNewLine]", "]"}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"Return", "[", 
         RowBox[{"tensorTrace", "[", 
          RowBox[{"state", ",", "TensorProduct"}], "]"}], "]"}], ";"}]}], 
      "\[IndentingNewLine]", "]"}], "\[IndentingNewLine]", "]"}]}], 
   "]"}]}]], "Input",
